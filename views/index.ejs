<!--笔记列表-->
<%- include header.ejs %>
<div class="container">
    <div class="row">

        <table class="table table-hover">
            <caption>我的笔记列表</caption>
            <thead>
            <tr>
                <th>标题</th>
                <th>标签</th>
                <th>日期</th>
                <th>操作</th>
            </tr>
            </thead>
            <tbody>
            <% notes.forEach(function(note){ %>
            <tr>
                <td>
                    <a href="/detail/<%= note._id %>"><%= note.title %></a>
                </td>
                <td><%= note.tag %></td>
                <td><%= note.createTime %></td>
                <td><a href="/editor/<%=note._id%>">修改</a> <a href="/detail/<%= note._id %>">查看</a> 删除</td>
            </tr>
            <% }); %>

            </tbody>

        </table>
        <ul class="pagination" id="pagination">12345</ul>
    </div>
</div>
<%- include footer.ejs %>